

<h2><%= link_to @league.name, league_path(@league.id)%></h2>
<ul>
  <!-- <li><%#= @creator.name %></li> -->
  <% @user_scores.each do |user_score| %>
    <li><%= user_score[:user_name] %> <%= user_score[:score] %></li>
  <% end %>
</ul>

<%= form_tag do %>
  <p>
    <%#= label_tag "Your wins", :class => "font" %>
    <%= number_field_tag :wins, type="Your wins", :class => "color"  %>
    <%= submit_tag "Update", :class => "updateButton" %>
  </p>
<% end %>
